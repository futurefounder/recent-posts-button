{
    "manifest_version": 3,
    "name": "LinkedIn Recent Posts Button Chrome Extension",
    "description": "Add a recent posts button to LinkedIn profile pages.",
    "version": "1.1",
    "host_permissions": [
      "https://www.linkedin.com/in/*"
    ],
    "content_scripts": [
      {
        "matches": ["https://www.linkedin.com/*"],
        "js": ["content.js"]
      }
    ],
    "icons": {
      "19": "icons/19x19.png",
      "38": "icons/38x38.png"
    }
  }
  